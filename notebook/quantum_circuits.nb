       RowBox[{"\[Lambda]", ",", "IntegerQ"}], "]"}]}], ",", "A_"}], "]"}], "/;", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"Unequal", "@@", "\[Lambda]"}], ")"}], "\[And]", 
    RowBox[{
     RowBox[{"Min", "[", "\[Lambda]", "]"}], "\[GreaterEqual]", "1"}], 
    "\[And]", 
    RowBox[{
     RowBox[{"Max", "[", "\[Lambda]", "]"}], "\[LessEqual]", "n"}], "\[And]", 
    
    RowBox[{
     RowBox[{"Dimensions", "[", "A", "]"}], "\[Equal]", 
     RowBox[{"{", 
      RowBox[{
       SuperscriptBox["2", "n"], ",", 
       SuperscriptBox["2", "n"]}], "}"}]}]}]}], ":=", 
  RowBox[{
   RowBox[{"IdentityMatrix", "[", 
    RowBox[{
     SuperscriptBox["2", "n"], ",", "SparseArray"}], "]"}], "+", 
   RowBox[{
    RowBox[{"Apply", "[", 
     RowBox[{"Dot", ",", 
      RowBox[{
       RowBox[{
        RowBox[{"op", "[", 
         RowBox[{"n", ",", "#", ",", "proj1"}], "]"}], "&"}], "/@", 
       "\[Lambda]"}]}], "]"}], ".", 
    RowBox[{"(", 
     RowBox[{"A", "-", 
      RowBox[{"IdentityMatrix", "[", 
       RowBox[{
        SuperscriptBox["2", "n"], ",", "SparseArray"}], "]"}]}], 
     ")"}]}]}]}]], "Input",
