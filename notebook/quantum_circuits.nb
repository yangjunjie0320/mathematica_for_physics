Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"sqrtSwapGate", "[", 
    RowBox[{"n_Integer", ",", 
     RowBox[{"{", 
      RowBox[{"j_Integer", ",", "k_Integer"}], "}"}]}], "]"}], "/;", 
   RowBox[{
    RowBox[{"1", "\[LessEqual]", "j", "\[LessEqual]", "n"}], "\[And]", 
    RowBox[{"1", "\[LessEqual]", "k", "\[LessEqual]", "n"}], "\[And]", 
    RowBox[{"j", "\[NotEqual]", "k"}]}]}], ":=", 
  RowBox[{
   RowBox[{
    FractionBox[
     RowBox[{"3", "+", "\[ImaginaryI]"}], "4"], 
    RowBox[{"IdentityMatrix", "[", 
     RowBox[{
      SuperscriptBox["2", "n"], ",", "SparseArray"}], "]"}]}], "+", 
   RowBox[{
    FractionBox[
     RowBox[{"1", "-", "\[ImaginaryI]"}], "4"], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{"xGate", "[", 
